<Window x:Class="Gestione_Negozio_Abbigliamento.Prodotti"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gestione_Negozio_Abbigliamento"
        mc:Ignorable="d"
        Title="Prodotti" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="14,51,0,0" Width="100">
            <Label HorizontalAlignment="Center" VerticalAlignment="Center" >Menu</Label>
            <Button x:Name="btnUtentepagProdotti" Click="btnUtentepagProdotti_Click">Utente</Button>
            <Button x:Name="btnHomePagepagProdotti" Click="btnHomePagepagProdotti_Click" >HomePage</Button>
            <Button x:Name="btnOrdinipagProdotti" Click="btnOrdinipagProdotti_Click" >Ordini</Button>


        </StackPanel>
    </Grid>
</Window>
